# Default profile for local development
[profile.default]
address = "localhost:7233"
namespace = "default"

# Profile for Temporal Cloud
[profile.cloud]
# Called "Regional Endpoint" in the Cloud UI; for example "us-east-1.aws.api.temporal.io:7233"
address = "CLOUD_REGIONAL_ENDPOINT"
namespace = "CLOUD_NAMESPACE"
api_key = "CLOUD_API_KEY"
